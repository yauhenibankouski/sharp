<div class="jumbotron d-flex align-items-center bg-dark text-light mb-0">
  <h2>
    <%= link_to booking_shared_training_plan_path(@booking, @shared_training_plan) do %>
      <i class="fas fa-angle-left pr-3 text-light"></i>
    <% end %>
  </h2>
  <h2><%= @shared_exercise.title %></h2>
</div>

<% if @shared_exercise.photo.key%>
<%= cl_image_tag @shared_exercise.photo.key, height: 300, width: 400, crop: :fill %>
<% end %>
<p><%= @shared_exercise.description %></p>
<p><%= @shared_exercise.technique %></p>
<p><%= @shared_exercise.sets %></p>
<p><%= @shared_exercise.repetitions %></p>



<%= link_to 'Upload', new_booking_shared_training_plan_shared_exercise_history_log_path(@booking, @shared_training_plan, @shared_exercise) %>
<%= link_to 'My Progress', booking_shared_training_plan_shared_exercise_history_logs_path(@booking, @shared_training_plan, @shared_exercise) %>
